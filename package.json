{
  "name": "team-sync",
  "version": "0.6.2-invite-user",
  "private": true,
  "description": "GitHub action to synchronize GitHub Teams with the contents of a teams document",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "NODE_OPTIONS=--openssl-legacy-provider ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prasadprabhu333/team-sync.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Prasad",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^2.2.0",
    "@octokit/rest": "^16.43.1",
    "@sindresorhus/slugify": "^0.11.0",
    "@types/js-yaml": "^3.12.6",
    "js-yaml": "^3.14.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  }
}
